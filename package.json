{
  "name": "js-sdk-starter",
  "description": "js sdk打包模板",
  "version": "1.0.0",
  "scripts": {
    "dev": "rollup -c -w",
    "reload": "livereload 'src/",
    "watch": "npm-run-all --parallel dev",
    "build": "cross-env NODE_ENV=production rollup -c",
    "build:test": "cross-env NODE_ENV=test rollup -c",
    "build:dev": "cross-env NODE_ENV=dev rollup -c"
  },
  "dependencies": {
    "@babel/core": "^7.15.0",
    "@babel/plugin-proposal-class-properties": "^7.14.5",
    "@babel/plugin-proposal-decorators": "^7.14.5",
    "@babel/plugin-transform-runtime": "^7.15.0",
    "@babel/preset-env": "^7.15.0",
    "babel-eslint": "^10.1.0",
    "core-js": "^3.16.4",
    "cross-env": "^5.2.0",
    "eslint": "^7.32.0",
    "eslint-config-standard": "12",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-jest": "^24.4.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.1.0",
    "eslint-plugin-standard": "^5.0.0",
    "npm-run-all": "^4.1.5",
    "rollup": "^1.12.0",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-eslint": "^5.0.0",
    "rollup-plugin-json": "^3.1.0",
    "rollup-plugin-livereload": "^0.6.0",
    "rollup-plugin-node-resolve": "^4.2.3",
    "rollup-plugin-postcss": "^3.1.6",
    "rollup-plugin-replace": "^2.1.0",
    "rollup-plugin-serve": "^0.6.1",
